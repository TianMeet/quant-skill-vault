{
  "name": "quant-skill-vault",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "postinstall": "prisma generate",
    "lint": "eslint",
    "db:up": "docker compose up -d",
    "db:down": "docker compose down",
    "db:migrate": "npx prisma migrate dev",
    "db:push": "npx prisma db push",
    "db:seed": "npx prisma db seed",
    "db:generate": "npx prisma generate",
    "db:generate:local": "set -a; source .env.local; set +a; npx prisma generate",
    "db:push:local": "set -a; source .env.local; set +a; npx prisma db push",
    "db:migrate:local": "set -a; source .env.local; set +a; npx prisma migrate dev",
    "db:migrate:local:create": "set -a; source .env.local; set +a; npx prisma migrate dev --create-only --name ${npm_config_name:-local_change}",
    "db:migrate:local:apply": "set -a; source .env.local; set +a; npx prisma migrate dev",
    "db:check:migration": "bash scripts/check-prisma-migration.sh",
    "tag:normalize": "node scripts/normalize-tags.js",
    "test": "vitest run",
    "test:unit": "vitest run src/lib/__tests__",
    "test:api": "vitest run src/app/api/__tests__",
    "test:e2e": "playwright test",
    "test:watch": "vitest",
    "format": "prettier --write .",
    "skill:init": "node scripts/init-local-skill.js",
    "skill:validate": "node scripts/quick-validate-skill.js",
    "skill:package": "node scripts/package-local-skill.js",
    "skill:sync:anthropic": "node scripts/sync-anthropic-official-skills.js",
    "skill:sync:anthropic:dry": "node scripts/sync-anthropic-official-skills.js --dry-run"
  },
  "prisma": {
    "seed": "npx tsx prisma/seed.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.74.0",
    "@prisma/client": "^5.22.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "archiver": "^7.0.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "js-yaml": "^4.1.1",
    "lucide-react": "^0.564.0",
    "mysql2": "^3.17.1",
    "next": "16.1.6",
    "prisma": "^5.22.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.4.1",
    "zod": "^4.3.6",
    "zustand": "^5.0.11"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "prisma",
      "@prisma/client",
      "@prisma/engines",
      "esbuild"
    ]
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@tailwindcss/postcss": "^4",
    "@types/adm-zip": "^0.5.7",
    "@types/archiver": "^7.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/supertest": "^6.0.3",
    "@vitejs/plugin-react": "^5.1.4",
    "adm-zip": "^0.5.16",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "prettier": "^3.8.1",
    "supertest": "^7.2.2",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^4.0.18"
  }
}
